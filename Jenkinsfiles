pipeline{
   agent any
   stages{
	stage('clean workspace')
	{
           steps{
              sh ' rm -r *'
		sh 'rm -r ../../../../www/html/* '
	   }
	}

        stage('git scm clone'){
             
	     steps{
                  
		  sh 'https://gitlab.com/karthikkandiyoor29/wedding.git -b master'

	     }
 
	}
	stage('deploy'){
              steps{
                sh ' mv wedding/* ../../../../www/html '
	      }
	}
     
   }
  
}

